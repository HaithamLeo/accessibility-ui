{
  "name": "react-a11y-kit",
  "author": "Riham AL Khamiasi",
  "license": "MIT",
  "description": "A comprehensive React accessibility widget that integrates seamlessly with React apps to enhance usability for all users. react-a11y-kit provides adjustable font sizes, alignment, dyslexia-friendly fonts, color and contrast controls, blue light filtering, brightness adjustment, zoom functionality, and more. Designed to help developers meet WCAG standards and create universally accessible web applications, react-a11y-kit empowers every visitor to customize their browsing experience for maximum comfort and inclusion.",
  "keywords": [
    "accessibility",
    "web-accessibility",
    "a11y",
    "React",
    "widget",
    "accessibility-ui",
    "Multilingual Support",
    "Adjust Font Size",
    "Align Text",
    "Dyslexia Font",
    "Font Weight",
    "Highlight Links",
    "Highlight Titles",
    "Letter Spacing",
    "Line Height",
    "Word Spacing",
    "BlueLight Filter",
    "Brightness Control",
    "DarkContrast Button",
    "HighContrast Button",
    "HighSaturation Button",
    "LightContrast Button",
    "LowSaturation Button",
    "Monochrome Button",
    "TextColor Picker",
    "Visual Impairment",
    "Zoom Button",
    "Big Cursor",
    "Reading Guide",
    "React Accessibility",
    "Accessibility for React",
    "Accessibility React component",
    "Accessibility component",
    "Accessibility widget",
    "React accessibility",
    "accessibility for React",
    "accessibility React component",
    "accessibility component",
    "accessibility widget",
    "WCAG compliance",
    "User-friendly web accessibility",
    "Inclusive web design",
    "React JS accessibility",
    "Accessible user interface",
    "Web inclusively tools",
    "User experience enhancement",
    "Web accessibility standards",
    "Universal web access solutions"
  ],
  "version": "1.0.2",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./dist/assets/main.css": "./dist/assets/main.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HaithamLeo/accessibility-ui.git"
  },
  "bugs": {
    "url": "https://github.com/HaithamLeo/accessibility-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "vite preview",
    "preview:demo": "vite preview --config vite.demo.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "release": "npm run build && npm publish --access public"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "glob": "^10.3.10",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.69.5",
    "typescript": "^5.6.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-lib-inject-css": "^1.3.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "dependencies": {
    "@types/i18n": "^0.13.10",
    "@uidotdev/usehooks": "^2.4.1",
    "classnames": "^2.3.2",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "immer": "^10.0.3",
    "lucide-react": "^0.553.0",
    "rc-slider": "^10.5.0",
    "react-colorful": "^5.6.1",
    "react-i18next": "^13.5.0",
    "react-select": "^5.8.0"
  }
}
